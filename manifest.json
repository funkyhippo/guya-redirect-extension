{
  "name": "Guya MD/NH Proxy",
  "description": "Redirects to the Guya proxy",
  "version": "1.0",
  "browser_specific_settings": {
    "gecko": {
      "id": "guya-redirect-extension@guya.moe"
    }
  },
  "permissions": ["activeTab", "tabs"],
  "background": {
    "scripts": ["script.js"]
  },
  "browser_action": {
    "default_title": "Redirect to Guya proxy",
    "default_icon": "logo.png"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "manifest_version": 2
}
